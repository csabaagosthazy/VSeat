@model IEnumerable<WebApp.ViewModels.OrderVM>
@{
    ViewData["Title"] = "Courier Page";
}

<div class="container-fluid h-100">
    <header class="page-header">
        <h2>My page</h2>
    </header>
    <section class="page-section">
        <nav class="sidenav">
            <ul>
                <li><a asp-area="" asp-controller="Courier" asp-action="Orders">My orders</a></li>
            </ul>
        </nav>

        <article class="page">
            <h2>Welcome @ViewBag.name</h2>
            <p>Here you can browse orders assigned to you</p>
        </article>
    </section>
</div>